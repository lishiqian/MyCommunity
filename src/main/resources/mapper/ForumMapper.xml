<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.lsq.community.mapper.ForumMapper">

    <resultMap id="ForumResultMap" type="com.lsq.community.pojo.Forum">
        <id property="id" column="id"></id>
        <result property="createTime" column="create_time"></result>
        <result property="lastUpdateTime" column="last_update_time"></result>
    </resultMap>

    <insert id="addForum" parameterType="com.lsq.community.pojo.Forum">
      INSERT INTO `forum` (`title`, `content`, `create_time`, `last_update_time`, `status`) VALUES (#{title}, #{content}, #{createDate}, #{lastUpdateTime}, #{status})
    </insert>

</mapper>